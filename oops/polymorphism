class Shape:
    def area(self):
        print("Area of an geometry")
    
class Circle:
    def __init__(self,radius):
        self.radius=radius
        self.c_area=22/7*self.radius * self.radius
    def area(self):
        print(f"Area of the circle: {self.c_area}")
        
class Rectangle:
    def __init__(self,length,breadth):
        self.length=length
        self.breadth=breadth
        self.r_area=self.length*self.breadth
    def area(self):
        print(f"Area of the rectangle: {self.r_area}")

        
quad=[Circle(7),Rectangle(2,4)]
for i in quad:
    i.area()
    
